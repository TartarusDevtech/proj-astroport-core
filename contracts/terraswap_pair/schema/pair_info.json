{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PairInfo",
  "type": "object",
  "required": [
    "asset_infos",
    "contract_addr",
    "end_time",
    "liquidity_token",
    "start_time"
  ],
  "properties": {
    "asset_infos": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/WeightedAssetInfo"
      },
      "maxItems": 2,
      "minItems": 2
    },
    "contract_addr": {
      "$ref": "#/definitions/HumanAddr"
    },
    "description": {
      "type": [
        "string",
        "null"
      ]
    },
    "end_time": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "liquidity_token": {
      "$ref": "#/definitions/HumanAddr"
    },
    "start_time": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "definitions": {
    "AssetInfo": {
      "anyOf": [
        {
          "type": "object",
          "required": [
            "token"
          ],
          "properties": {
            "token": {
              "type": "object",
              "required": [
                "contract_addr"
              ],
              "properties": {
                "contract_addr": {
                  "$ref": "#/definitions/HumanAddr"
                }
              }
            }
          }
        },
        {
          "type": "object",
          "required": [
            "native_token"
          ],
          "properties": {
            "native_token": {
              "type": "object",
              "required": [
                "denom"
              ],
              "properties": {
                "denom": {
                  "type": "string"
                }
              }
            }
          }
        }
      ]
    },
    "HumanAddr": {
      "type": "string"
    },
    "Uint128": {
      "type": "string"
    },
    "WeightedAssetInfo": {
      "type": "object",
      "required": [
        "end_weight",
        "info",
        "start_weight"
      ],
      "properties": {
        "end_weight": {
          "$ref": "#/definitions/Uint128"
        },
        "info": {
          "$ref": "#/definitions/AssetInfo"
        },
        "start_weight": {
          "$ref": "#/definitions/Uint128"
        }
      }
    }
  }
}
